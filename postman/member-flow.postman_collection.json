        {
          "name": "members-list",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Save first member id from list', function () {",
                  "  const body = pm.response.json();",
                  "  pm.expect(body).to.have.property('data');",
                  "  pm.expect(body.data).to.be.an('array');",
                  "  const targetEmail = pm.collectionVariables.get('adminCreatedEmail') || pm.environment.get('adminCreatedEmail');",
                  "  const found = body.data.find((x) => x && x.email === targetEmail);",
                  "  const selected = found || body.data[0];",
                  "  if (selected && selected.id) {",
                  "    pm.collectionVariables.set('memberId', selected.id);",
                  "    pm.environment.set('memberId', selected.id);",
                  "    pm.collectionVariables.set('uploadedBy', selected.id);",
                  "    pm.environment.set('uploadedBy', selected.id);",
                  "  }",
                  "});"
                ]
              }
            }
